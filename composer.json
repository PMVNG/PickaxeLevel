{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "pocketmine/pocketmine-mp": "^5.0.0",
        "vecnavium/formsui": "master",
        "taylor-pm-pl/lockeditem": "master"
    },
    "require-dev": {
        "phpstan/phpstan": "1.10.14",
        "friendsofphp/php-cs-fixer": "^3.16"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "vecnavium/formsui",
                "version": "master",
                "autoload": {
                    "classmap": [
                        "src/"
                    ]
                },
                "source": {
                    "url": "https://github.com/Vecnavium/FormsUI",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "taylor-pm-pl/lockeditem",
                "version": "master",
                "autoload": {
                    "classmap": [
                        "src/"
                    ]
                },
                "source": {
                    "url": "https://github.com/Taylor-pm-pl/LockedItem",
                    "type": "git",
                    "reference": "pm5"
                }
            }
        }
    ],
    "autoload": {
        "classmap": [
            "src"
        ]
    }
}
